
<% if @is_followed %>
$('#follow_<%= @user.id%>').html("UnFollow");
console.log($('#follow_<%= @user.id%>').html);
noty({text: 'User was Followed', type: 'success', timeout: 3500, progressBar: true});

<% else %>

$('#follow_<%= @user.id%>').html("Follow");

noty({text: 'User was UnFollowed', type: 'success', timeout: 3500, progressBar: true});

<% end %>